{
  "manifest_version": 2,

  "name": "Remove Element From Dom",
  "description": "Easily remove elements from the current page's DOM including tags, ids and classes.",
  "version": "1.0",
  "icons": {  
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png" 
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "img/icon16.png",
    "default_popup": "popup.html"
  }
}